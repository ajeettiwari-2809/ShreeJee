            "name": "Commit-0",
            "content": "<mat-nav-list>\r\n    <div class=\"logo py-4\">\r\n      <!-- <img src=\"./assets/logo.svg\" alt=\"Company Logo\" /> -->\r\n      <b style=\"color: rgb(254, 252, 255);size: 20px;font-size: 15px;\">PRISM <span style=\"color: black;\">JOHNSON</span> LIMITED</b>\r\n    </div>\r\n\r\n   \r\n    <mat-divider></mat-divider>\r\n\r\n    <div class=\"container\" style=\"background-color: #3f51b5;\">\r\n      <a mat-list-item routerLinkActive=\"list-item-active\"  target=\"_self\" (click)=\"connecttoASMS()\" \r\n     \r\n      style=\" font-weight: bold;\r\n      font-size: 15px;\r\n      font-family: math;\">\r\n        <mat-icon>login</mat-icon> Connect to ASMS: 1.0\r\n      </a>\r\n     </div>\r\n\r\n     \r\n     \r\n\r\n    <h2 matSubheader class=\"mt-2\" style=\"color: #fbfbfb;font-size: 12px; font-weight: 400;\">APPLICATIONS</h2>\r\n        \r\n    <a mat-list-item routerLinkActive=\"list-item-active\" routerLink=\"/home\">\r\n      <mat-icon>home</mat-icon> Home\r\n    </a>\r\n\r\n    \r\n\r\n    <!-- <a mat-list-item routerLinkActive=\"list-item-active\" routerLink=\"/openWebsite\">\r\n      <mat-icon>home</mat-icon> WebsiteOpen\r\n\r\n      href=\"https://www.prismcement.com/\"\r\n    </a> -->\r\n\r\n    <!-- <a mat-list-item routerLinkActive=\"list-item-active\" routerLink=\"/dashboard\">\r\n      <mat-icon>dashboard</mat-icon> Dashboard\r\n    </a>\r\n    <a mat-list-item routerLinkActive=\"list-item-active\" routerLink=\"/vehicle-application\"> <mat-icon>Request</mat-icon> Vehicle Request1 </a>\r\n    \r\n\r\n    <a mat-list-item routerLinkActive=\"list-item-active\" routerLink=\"/DriversDetails\"> <mat-icon>Request</mat-icon> Driver Details </a>\r\n    <a mat-list-item routerLinkActive=\"list-item-active\" routerLink=\"/VehicleOutDetails\"> <mat-icon>vehicle</mat-icon> Vehicle Out </a> -->\r\n\r\n    <!-- <a mat-list-item routerLinkActive=\"list-item-active\" routerLink=\"/vehicle-application\"> <mat-icon>Request</mat-icon> Vehicle Request </a> -->\r\n     <!-- vehiclerequestdetals //VehicleApprovalHOD-->\r\n\r\n     <!-- <button mat-icon-button [matMenuTriggerFor]=\"menu\" aria-label=\"Example icon-button with a menu\">\r\n      APPROVAL\r\n      <mat-icon>more_vert</mat-icon>\r\n    </button>\r\n    <mat-menu #menu=\"matMenu\">\r\n      <button mat-menu-item>\r\n        <mat-icon>dialpad</mat-icon>\r\n        <span>Redial</span>\r\n      </button>\r\n      <button mat-menu-item disabled>\r\n        <mat-icon>voicemail</mat-icon>\r\n        <span>Check voice mail</span>\r\n      </button>\r\n      <button mat-menu-item>\r\n        <mat-icon>notifications_off</mat-icon>\r\n        <span>Disable alerts</span>\r\n      </button>\r\n    </mat-menu> -->\r\n\r\n\r\n\r\n     <a mat-list-item routerLinkActive=\"list-item-active\" routerLink=\"/vehiclerequestdetals\" *ngIf=\"getUserRole() \"> <mat-icon>directions_car</mat-icon> Vehicle Requisition</a> \r\n\r\n     \r\n<!-- \r\n     <a mat-list-item routerLinkActive=\"list-item-active\" *ngIf=\"getAdminrole()\" routerLink=\"/add_vehicle_by_Admin\" > <mat-icon>directions_car</mat-icon> Add Vehicle</a> \r\n      \r\n     <a mat-list-item routerLinkActive=\"list-item-active\" *ngIf=\"getAdminrole()\" routerLink=\"/add_driver_by_Admin\" > <mat-icon>directions_car</mat-icon> Add Driver</a>  -->\r\n\r\n     <!-- <a mat-list-item routerLinkActive=\"list-item-active\" routerLink=\"/Vehicle_Allotement\" > <mat-icon>directions_car</mat-icon> Vehicle Allotment</a> \r\n    \r\n     <a mat-list-item routerLinkActive=\"list-item-active\" routerLink=\"/Vehicle_Bill_Processing\" > <mat-icon>directions_car</mat-icon> Bill Process</a>  -->\r\n\r\n\r\n     <a mat-list-item routerLinkActive=\"list-item-active\"  *ngIf=false id=\"ibutton\"> <mat-icon> {{iconname}} </mat-icon> <button (click)=\"checkselect()\" style=\"background: transparent;border: none;color: white;\" >APPROVAL</button> </a> \r\n     <!-- buttonstatus -->\r\n     <div *ngIf=\"buttonstatus\" style=\"background-color: rgb(118, 118, 177);\"> \r\n\r\n      <p>\r\n       \r\n        <a  mat-list-item  routerLinkActive=\"list-item-active\" routerLink=\"/vehicleappravalhodlist\" > Vehicle Approval HOD</a>\r\n      </p>\r\n      <p>\r\n       \r\n        <a  mat-list-item  routerLinkActive=\"list-item-active\" routerLink=\"/fdfdfd\" > Vehicle Approval </a>\r\n      </p>\r\n    \r\n    </div>\r\n\r\n    \r\n    <!-- <a mat-list-item routerLinkActive=\"list-item-active\" routerLink=\"/VehicleEntryForm\"  *ngIf=\"getSecurityrole()||getHODSecurityrole()||getAdminrole()\"> <mat-icon> directions_car</mat-icon> Vehicle Gate In/Out </a> -->\r\n    <!-- <a mat-list-item routerLinkActive=\"list-item-active\" routerLink=\"/VehicleReportStatus\"  *ngIf=\"getSecurityrole()||getHODSecurityrole()\"> <mat-icon> directions_car</mat-icon> Vehicle Report Status </a> -->\r\n\r\n   \r\n\r\n\r\n    <!-- blockstatus -->\r\n   \r\n   <!--  <a mat-list-item routerLinkActive=\"list-item-active\"  *ngIf=\" false|| getHODSecurityrole() || getAdminrole() \" id=\"ibutton1\"> <mat-icon>{{iconname1}}</mat-icon> <button (click)=\"checkselect1()\" style=\"background: transparent;border: none;color: white;\" >BLOCK</button></a> \r\n    \r\n    <div *ngIf=\"buttonstatus1\" style=\"background-color: rgb(118, 118, 177);\"> \r\n\r\n      <p>\r\n       \r\n        <a  mat-list-item  routerLinkActive=\"list-item-active\" routerLink=\"/VehicleBlock\" > Vehicle Block</a>\r\n      </p>\r\n      <p>\r\n       \r\n        <a  mat-list-item  routerLinkActive=\"list-item-active\" routerLink=\"/DriverBlock\" > Driver Block </a>\r\n      </p>\r\n    \r\n    </div> -->\r\n    \r\n\r\n    <!-- <a mat-list-item routerLinkActive=\"list-item-active\" routerLink=\"/vehiclerequistionlist\"\r\n    *ngIf=\"getSecurityrole()\"> <mat-icon>directions_car</mat-icon> Colony Vehicle Details </a>  -->\r\n\r\n    <!-- <a mat-list-item routerLinkActive=\"list-item-active\" routerLink=\"/guesthouseDetails\"\r\n    *ngIf=\"getHODSecurityrole()\"> <