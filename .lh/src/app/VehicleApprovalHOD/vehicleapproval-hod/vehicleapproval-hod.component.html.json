      \r\n      </div>\r\n    </div>\r\n\r\n\r\n\r\n    <div class=\"container\">\r\n      <div class=\"row justify-content-center\">\r\n        <!-- <div class=\"col-6\">\r\n          <mat-radio-group style=\"padding: 20px;\" class=\"col-lg-12 col-md-12 col-sm-12 \" aria-label=\"Select an option\">\r\n            <mat-radio-button style=\"padding-right: 30px;\" value=\"0\" checked (click)=\"getVehicleRequestList(0)\">Pending\r\n              </mat-radio-button>\r\n\r\n            <mat-radio-button #valueRadio value=\"1\" (click)=\"getVehicleRequestList(1)\">Approved </mat-radio-button>\r\n          </mat-radio-group>\r\n\r\n         \r\n        </div> -->\r\n        <div class=\"col-6\">\r\n          <!-- <button mat-raised-button color=\"warn\"  style=\"width: 150px; height: 30px; float: right; margin-top: 10px; margin-right: 0px; margin-left: 30px;\" class=\"col-lg-3 col-md-3 col-sm-1\" (click)=\"getSearch()\">  <mat-icon>search</mat-icon>\r\n              \r\n              </button> -->\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n\r\n\r\n\r\n\r\n    <!-- <mat-icon>keyboard_arrow_down</mat-icon> -->\r\n\r\n  </mat-card>\r\n  <mat-divider></mat-divider>\r\n  <br>\r\n\r\n  <form [formGroup]=\"VehicleRequestdetails\">\r\n\r\n    <div class=\"row\" *ngIf=\"getseachlist\">\r\n      <mat-form-field class=\"col-lg-3 col-md-3 col-sm-6\" appearance=\"fill\">\r\n        <mat-label>From Date</mat-label>\r\n        <input formControlName=\"fromDate1\" matInput [matDatepicker]=\"picker\" [(ngModel)]=\"fromDate\"\r\n          placeholder=\"From Date\">\r\n        <mat-datepicker-toggle matSuffix [for]=\"picker\"></mat-datepicker-toggle>\r\n        <mat-datepicker #picker></mat-datepicker>\r\n      </mat-form-field>\r\n\r\n\r\n      <mat-form-field class=\"col-lg-3 col-md-3 col-sm-6\" appearance=\"fill\">\r\n        <mat-label>To Date</mat-label>\r\n        <input formControlName=\"toDate1\" [min]=\"fromDate\" matInput [matDatepicker]=\"picker1\" placeholder=\"To Date\">\r\n        <mat-datepicker-toggle matSuffix [for]=\"picker1\"></mat-datepicker-toggle>\r\n        <mat-datepicker #picker1></mat-datepicker>\r\n      </mat-form-field>\r\n\r\n      <button mat-raised-button color=\"warn\"\r\n\r\n        style=\"width: 150px; height: 50px; float: right; margin-top: 10px; margin-right: 0px; margin-left: 30px;\"\r\n        class=\"col-lg-3 col-md-3 col-sm-1\" type=\"submit\"\r\n        (click)=\"getVehicleRequestList(this.Getlist.value)\"\r\n        > <mat-icon>search</mat-icon></button>\r\n     \r\n\r\n      <mat-divider></mat-divider>\r\n    </div>\r\n\r\n    <div class=\"col-lg-12 col-mg-12 col-sm-12\" *ngFor=\"let list of VehicleList |filter : searchKeyWord; \">\r\n\r\n      <mat-card class=\"alldata\">\r\n\r\n        <button mat-mini-fab class=\"float-right\" style=\"float: right;\"\r\n          [routerLink]='[\"/VehicleRequestDetailsById/\" + list.id,{HODApproval:\"true\",activeradio:radiobuttonActive}]'>\r\n          <!-- <mat-icon aria-label=\"\" >view</mat-icon> VehicleEntrylistbyid -->\r\n          <mat-icon>visibility</mat-icon>\r\n        </button>\r\n\r\n        <div class=\"row\" style=\"color: rgb(43, 37, 37);font-size: 14px;\">\r\n          <div class=\"col-lg-4 col-md-4 col-sm-6\"><span class=\"small\"><b>Vehicle Type:&nbsp; </b></span>{{ list.type\r\n            }} </div>\r\n            <div class=\"col-lg-4 col-md-4 col-sm-6\"><span class=\"small\"><b>Vehicle Code:&nbsp;</b></span>{{\r\n              list.appCode }} </div>\r\n          <div class=\"col-lg-4 col-md-4 col-sm-6\"><span class=\"small\"><b>EmpName:&nbsp;</b></span>{{\r\n            list.empname }} </div>\r\n\r\n          <div class=\"col-lg-4 col-md-4 col-sm-6\"><span class=\"small\"><b>Vehicle Request On:&nbsp; </b></span>{{\r\n            list.vehiclereqon | date:'dd/MM/yyyy hh:mm a'}} </div>\r\n            <div class=\"col-lg-4 col-md-4 col-sm-6\"><span class=\"small\"><b>Vehicle Request At:&nbsp; </b></span>{{\r\n              list.vehiclereqat }} </div>\r\n\r\n           \r\n          <!-- <div class=\"col-lg-4 col-md-4 col-sm-6\"  *ngIf=\"getinoutstatus\"><span class=\"small\"><b>Vehicle getOut:&nbsp;</b></span>{{\r\n            list.gateOutAt |  date:'dd/MM/yyyy hh:mm a'}} </div> -->\r\n          <div class=\"col-lg-4 col-md-4 col-sm-6\"><span class=\"small\"><b>Person:&nbsp;</b></span>{{\r\n            list.noofperson}} </div>\r\n          <div class=\"col-lg-4 col-md-4 col-sm-6\"><span class=\"small\"><b>Visit Place\r\n                :&nbsp;</b></span>{{list.visitingplace }} </div>\r\n          <div class=\"col-lg-4 col-md-4 col-sm-6\"><span class=\"small\"><b>Purpose:&nbsp;</b></span>{{\r\n            list.purpose}} </div>\r\n\r\n          <div class=\"col-lg-4 col-md-4 col-sm-6\"><span class=\"small\"><b>Expected Return Date:&nbsp;</b></span>{{\r\n            list.expectedreturnat}} </div>\r\n          <!-- <div class=\"col-lg-4 col-md-4 col-sm-6\"><span class=\"small\"><b>Driver Licence:&nbsp;</b></span>{{\r\n            list.driverLicence }} </div> -->\r\n\r\n        </div>\r\n\r\n      </mat-card>\r\n      <mat-divider></mat-divider>\r\n\r\n    </div>\r\n    <div *ngIf=\"VehicleList.length == 0\">\r\n\r\n      <h5 style=\"color: red; text-align: center;\"> No record available</h5>\r\n    </div>\r\n    <div class=\"row\">\r\n      <mat-paginator [length]=\"VehilceRequestlist.totalRecords\" [pageSize]=\"VehilceRequestlist.pageSize\"\r\n        [pageSizeOptions]=\"VehilceRequestlist.pageSizeOptions\" [pageIndex]=\"VehilceRequestlist.pageIndex\"\r\n        (page)=\"onChangedPage($event)\" [showFirstLastButtons]=\"true\" *ngIf=\"VehilceRequestlist.totalRecords >= 0 \">\r\n      </mat-paginator>\r\n    </div>\r\n\r\n  </form>\r\n</div>\r\n\r\n<!-- <div class=\"dialog_scrim\" *ngIf=\"openMenu\">\r\n  \r\n</div> -->\r\n\r\n\r\n\r\n\r\n<!-- <button mat-fab color=\"primary\" (click)=\"clickMenu()\"\r\n    class=\"menu-button\"\r\n    [class.mat-elevation-z2]=\"!isOver\"\r\n    [class.mat-elevation-z8]=\"is