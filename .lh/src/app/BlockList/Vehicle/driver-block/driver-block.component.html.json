            "content": "\r\n<div class=\"container\">\r\n    <h5>\r\n      <header>Driver Block Status</header>\r\n    </h5>\r\n    \r\n  \r\n    <mat-card>\r\n  \r\n     \r\n      <div class=\"container\">\r\n        <div class=\"row justify-content-center\">\r\n          <div class=\"col-6\">\r\n            <input type=\"text\" class=\"form-control\" placeholder=\"Search\" >\r\n          </div>\r\n          <div class=\"col-4\">\r\n            <button mat-raised-button color=\"warn\" style=\"float: right;margin: auto;\" class=\"col-lg-6 col-md-3 col-sm-1\" (click)=\"clickMenu()\"\r\n             >\r\n  \r\n              <span class=\" m-2\"> + Driver Block</span></button>\r\n          </div>\r\n          <div class=\" col-2\" >\r\n            <button mat-raised-button color=\"warn\"  routerLink=\"/add_vehicle_by_Admin\" > <mat-icon>add_box</mat-icon> Add Driver</button> \r\n             </div>\r\n        </div>\r\n      </div>\r\n  \r\n  \r\n  \r\n      <div class=\"container\">\r\n        <div class=\"row justify-content-center\">\r\n          <div class=\"col-6\">\r\n            <mat-radio-group style=\"padding: 20px;\" class=\"col-lg-12 col-md-12 col-sm-12 \" aria-label=\"Select an option\">\r\n              <mat-radio-button style=\"padding-right: 30px;\" value=\"0\" checked  (click)=\"getVehcilBlockedUnblockedlist(0)\">Blocked\r\n                </mat-radio-button>\r\n  \r\n              <mat-radio-button #valueRadio value=\"1\"  (click)=\"getVehcilBlockedUnblockedlist(1)\">UnBlocked</mat-radio-button>\r\n            </mat-radio-group>\r\n  \r\n           \r\n          </div>\r\n          <div class=\"col-6\">\r\n           \r\n          </div>\r\n        </div>\r\n      </div>\r\n  \r\n  \r\n  \r\n  \r\n  \r\n    \r\n  \r\n    </mat-card>\r\n    <mat-divider></mat-divider>\r\n    <br>\r\n    <form>\r\n  \r\n      \r\n      <div class=\"col-lg-12 col-mg-12 col-sm-12\" *ngFor=\"let list of _getVehiclBloclList \" id=\"_getVehiclBloclList\">\r\n  \r\n        <mat-card class=\"alldata\">\r\n  \r\n          <button mat-mini-fab class=\"float-right\" style=\"float: right;\"\r\n            (click)=\"UpdateStatus(list.id)\">\r\n            <!-- <mat-icon aria-label=\"\" >view</mat-icon> VehicleEntrylistbyid -->\r\n            <mat-icon>{{vehicleiconstatus}}</mat-icon>\r\n          </button>\r\n  \r\n          <div class=\"row\" style=\"color: rgb(43, 37, 37);font-size: 14px;\" >\r\n            <div class=\"col-lg-4 col-md-4 col-sm-6\"><span class=\"small\"><b>Driver Licence No:&nbsp; </b>{{list.driverLicenceNo}}</span> </div>\r\n  \r\n            <div class=\"col-lg-4 col-md-4 col-sm-6\"><span class=\"small\"><b>Driver Blocked Data:&nbsp;</b>{{list.blacklistdate |date}}</span></div>\r\n  \r\n            <div class=\"col-lg-4 col-md-4 col-sm-6\"><span class=\"small\"><b>Remark:&nbsp; </b>{{list.remark}}</span> </div>\r\n  \r\n            <div class=\"col-lg-4 col-md-4 col-sm-6\"  ><span class=\"small\"><b>Blocked By:&nbsp;</b>{{list.blacklistBy}}</span> </div>\r\n\r\n           \r\n\r\n\r\n           \r\n          </div>\r\n          \r\n  \r\n        \r\n        </mat-card>\r\n\r\n\r\n        <mat-divider></mat-divider>\r\n  \r\n      </div>\r\n     \r\n  \r\n    </form>\r\n  </div>\r\n  \r\n  <div class=\"dialog_scrim\" *ngIf=\"openMenu\">\r\n  \r\n  </div>\r\n  \r\n  \r\n  \r\n  \r\n  \r\n  \r\n  \r\n  \r\n  "
